{
    "logging": {
        "level": "INFO",
        "format": "\u001b[30;1m[ %(levelname)s \u001b[30;1m] %(datetime)s \u001b[30;1m| %(message)s",
        "datetimeFormat": " %a %d. %b %Y  %H:%M:%S.%f",
        "colors": {
            "DEBUG": "\u001b[34m",
            "INFO": "\u001b[32m",
            "WARN": "\u001b[33m",
            "ERROR": "\u001b[31m",
            "CRITICAL": "\u001b[31;1m",
            "RESET": "\u001b[0m",
            "DATETIME": "\u001b[36;1m",
            "MESSAGE": "\u001b[37m"
        }
    },
    "paths": {
        "log_file": "logs/app.log"
    },
    "libLogging": true
}